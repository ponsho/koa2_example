{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "node": true,
    "mocha": true
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1,
    "import/no-extraneous-dependencies": [2, {}],
    "no-param-reassign": [2, { "props": false }]
  }
}
